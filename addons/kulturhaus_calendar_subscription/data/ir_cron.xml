<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron job to cleanup unused tokens -->
        <record id="ir_cron_cleanup_calendar_tokens" model="ir.cron">
            <field name="name">Calendar: Cleanup Unused Subscription Tokens</field>
            <field name="model_id" ref="model_calendar_subscription_token"/>
            <field name="state">code</field>
            <field name="code">model.cleanup_old_unused_tokens()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>