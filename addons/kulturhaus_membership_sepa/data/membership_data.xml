<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for SEPA Mandate IDs -->
        <record id="seq_sepa_mandate" model="ir.sequence">
            <field name="name">SEPA Mandate</field>
            <field name="code">sepa.mandate</field>
            <field name="prefix">%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequence for SEPA Batch References -->
        <record id="seq_sepa_batch" model="ir.sequence">
            <field name="name">SEPA Batch</field>
            <field name="code">sepa.batch</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>